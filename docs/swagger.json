    "definitions": {
        "models.Itinerary": {
            "type": "object",
            "required": [
                "title"
            ],
            "properties": {
                "creationDate": {
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                },
                "description": {
                    "type": "string",
                    "example": "Summer vacation in Spain"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "notes": {
                    "type": "string",
                    "example": "I want to enjoy the nightlife"
                },
                "ownerId": {
                    "type": "integer",
                    "example": 1
                },
                "title": {
                    "type": "string",
                    "example": "Trip to Spain"
                },
                "travelDestinations": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.ItineraryTravelDestination"
                    }
                },
                "updateDate": {
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                }
            }
        },
        "models.ItineraryFileJob": {
            "type": "object",
            "properties": {
                "asyncTaskId": {
                    "description": "Optional, async task ID from task manager",
                    "type": "string",
                    "example": "e2467dd0-db8a-49db-a5cb-9474f8e63933"
                },
                "creationDate": {
                    "description": "Status can be \"running\", \"completed\", \"failed\", or \"stopped\"",
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                },
                "endDate": {
                    "description": "EndDate is set when the job ends",
                    "type": "string",
                    "example": "2024-06-01T00:01:00Z"
                },
                "fileManager": {
                    "description": "Optional, used for file management",
                    "type": "string",
                    "example": "local"
                },
                "filepath": {
                    "description": "Optional, used for file storage",
                    "type": "string",
                    "example": "/path/to/file.txt"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "itineraryId": {
                    "description": "ItineraryID is the ID of the itinerary associated with this job",
                    "type": "integer",
                    "example": 123
                },
                "startDate": {
                    "description": "CreationDate is set when the job is created",
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                },
                "status": {
                    "type": "string",
                    "example": "completed"
                },
                "statusDescription": {
                    "type": "string",
                    "example": "Job completed successfully"
                }
            }
        },
        "models.ItineraryTravelDestination": {
            "type": "object",
            "required": [
                "arrivalDate",
                "city",
                "country",
                "departureDate"
            ],
            "properties": {
                "arrivalDate": {
                    "type": "string",
                    "example": "2024-07-01T00:00:00Z"
                },
                "city": {
                    "type": "string",
                    "example": "Madrid"
                },
                "country": {
                    "type": "string",
                    "example": "Spain"
                },
                "creationDate": {
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                },
                "departureDate": {
                    "type": "string",
                    "example": "2024-07-05T00:00:00Z"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "itineraryId": {
                    "type": "integer",
                    "example": 1
                },
                "updateDate": {
                    "type": "string",
                    "example": "2024-06-01T00:00:00Z"
                }
            }
        },
        "routes.CreateItineraryRequest": {
            "type": "object",
            "required": [
                "destinations",
                "title"
            ],
            "properties": {
                "description": {
                    "type": "string",
                    "example": "Summer vacation in Spain"
                },
                "destinations": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/routes.DestinationItem"
                    }
                },
                "notes": {
                    "type": "string",
                    "example": "I want to enjoy the nightlife"
                },
                "title": {
                    "type": "string",
                    "example": "Trip to Spain"
                }
            }
        },
